{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-react"
    ],
    "linterOptions": {
        "exclude": [
            "src/chrome-extension-async.d.ts"
        ]
    },
    "jsRules": {
        "object-literal-sort-keys": false
    },
    "rules": {
        "no-console": false,
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false

    },
    "rulesDirectory": []
}